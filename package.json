    "webvr-boilerplate": "MetaCDN/webvr-boilerplate",
    "webvr-polyfill": "^0.9.20",
    "dashjs": "^2.2.0",
    "videojs-contrib-dash": "^2.5.1",
    "express": "4.13.4",
    "express-http-proxy": "0.6.0",
